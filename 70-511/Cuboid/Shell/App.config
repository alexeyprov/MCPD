<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.diagnostics>
        <sources>
            <source name="System.Windows.Data" switchValue="Information" >
                <listeners>
                    <add type="System.Diagnostics.TextWriterTraceListener" initializeData="trace.txt" name="wpfDataListener" />
                </listeners>
            </source>
        </sources>
        <trace autoflush="true" />
    </system.diagnostics>
</configuration>