<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/Website.css")" rel="stylesheet" 
        type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" 
        type="text/javascript"></script>
</head>
<body>
    <div id="header">
        <h1>
            <a href="/">TREY RESEARCH</a></h1>
        <ul id="navlist">
            <li><a href="@Url.Content("~")" id="current">Home</a></li>
            <li>@Html.ActionLink("Products", "Index", "StoreManager")</li>
            <li>@Html.ActionLink("Customers", "Index", "Customers")</li>
            <li>@Html.ActionLink("Audit Log", "Index", "Audit")</li>
            <li>@Html.ActionLink("Diagnostics Log", "Index", "Diagnostics")</li>
        </ul>
    </div>
    <div id="main">
        @RenderBody()
    </div>
</body>
</html>
